  "tokenColors": [
    {
      "scope": [
        "keyword.control.go",
        "keyword.package.go",
        "keyword.import.go",
        "keyword.func.go",
        "keyword.struct.go",
        "keyword.interface.go"
      ],
      "settings": {
        "foreground": "#9F7AEA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string.quoted.double.go", "string.quoted.raw.go"],
      "settings": {
        "foreground": "#2F855A"
      }
    },
    {
      "scope": ["string.template", "string.interpolated"],
      "settings": {
        "foreground": "#2F855A",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric.go",
      "settings": {
        "foreground": "#A17646"
      }
    },
    {
      "scope": "entity.name.function.go",
      "settings": {
        "foreground": "#2B6CB0"
      }
    },
    {
      "scope": "storage.type.go",
      "settings": {
        "foreground": "#9F7AEA"
      }
    },
    {
      "scope": ["comment.line.double-slash.go", "comment.block.go"],
      "settings": {
        "foreground": "#718096",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#4A5568",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#718096",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["entity.name.type.type-parameter.ts"],
      "settings": {
        "foreground": "#9F7AEA",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.template.cpp", "storage.type.template.cpp"],
      "settings": {
        "foreground": "#9F7AEA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "constant.other.symbol.ruby",
        "punctuation.section.block.begin.ruby",
        "punctuation.section.block.end.ruby"
      ],
      "settings": {
        "foreground": "#2F855A"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#9F7AEA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#2B6CB0"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "string.quoted",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#2F855A"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#A17646"
      }
    },
    {
      "scope": [
        "variable",
        "identifier",
        "variable.other.readwrite",
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#1A202C"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#2B6CB0"
      }
    },
    {
      "scope": ["entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#9F7AEA"
      }
    },
    {
      "scope": ["entity.name.tag", "meta.tag.sgml", "meta.tag.html"],
      "settings": {
        "foreground": "#2B6CB0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A17646"
      }
    },
    {
      "scope": [
        "entity.name.tag.css",
        "meta.selector.css",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#2B6CB0"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",

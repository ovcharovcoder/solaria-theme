    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#7BB8F0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#FFCA28"
      }

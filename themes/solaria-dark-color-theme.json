    },
    {
      "scope": [
        "variable.language.this.cs",
        "variable.language.this.java",
        "variable.language.this.cpp"
      ],
      "settings": {
        "foreground": "#C4A1F8",
        "fontStyle": "italic"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": "#E6ECF5",
    "parameter": "#FFCA28",
    "property": "#E6ECF5",
    "function": "#7BB8F0",
    "method": "#7BB8F0",
    "class": "#C4A1F8",
    "interface": "#C4A1F8",
    "struct": "#C4A1F8",
    "enum": "#FF4D4D",
    "typeParameter": "#C4A1F8",
    "namespace": "#7BB8F0",
    "macro": "#FFCA28",
    "modifier": "#C4A1F8",
    "regexp": "#68D391",
    "decorator": "#FF4D4D",

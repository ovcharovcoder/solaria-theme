	"tokenColors": [
		{
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#7c8aa3",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant.other.symbol.ruby",
				"punctuation.section.block.begin.ruby",
				"punctuation.section.block.end.ruby"
			],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"foreground": "#C4A1F8",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.language.boolean"
			],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"string",
				"constant.other.symbol",
				"string.quoted",
				"string.regexp"
			],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": ["variable", "variable.other.readwrite", "variable.parameter"],
			"settings": {
				"foreground": "#E6ECF5"
			}
		},
		{
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#7BB8F0"
			}
		},
		{
			"scope": ["entity.name.type", "support.class", "support.type"],
			"settings": {
				"foreground": "#A78DEA"
			}
		},
		{
			"scope": ["entity.name.tag", "meta.tag.sgml", "meta.tag.html"],
			"settings": {
				"foreground": "#7BB8F0",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"meta.selector.css",
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#5AA3E6"
			}
		},
		{
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss"
			],
			"settings": {
				"foreground": "#A78DEA"
			}
		},
		{
			"scope": ["variable.scss", "variable.other.sass"],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": ["entity.name.function.scss", "support.function.mixin.scss"],
			"settings": {
				"foreground": "#F87979"
			}
		},
		{
			"scope": ["variable.other.readwrite.js", "identifier.js"],
			"settings": {
				"foreground": "#E6ECF5"
			}
		},
		{
			"scope": [
				"punctuation.section.block.begin",
				"punctuation.section.block.end",
				"punctuation.section.array.begin",
				"punctuation.section.array.end",
				"punctuation.definition.block",
				"punctuation.definition.array",
				"punctuation.section.parens.begin",
				"punctuation.section.parens.end",
				"punctuation.definition.parameters.begin",
				"punctuation.definition.parameters.end",
				"meta.brace.round",
				"meta.brace.square",
				"meta.brace.curly",
				"punctuation.definition.begin",
				"punctuation.definition.end"
			],
			"settings": {
				"foreground": "#7BB8F0"
			}
		},
		{
			"scope": [
				"markup.heading.markdown",
				"markup.heading.setext.1.markdown",
				"markup.heading.setext.2.markdown"
			],
			"settings": {
				"foreground": "#F28B82",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.bold.markdown", "markup.bold"],
			"settings": {
				"foreground": "#F7C978",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["markup.italic.markdown", "markup.italic"],
			"settings": {
				"foreground": "#F7C978",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["markup.inline.raw.markdown", "markup.raw.block.markdown"],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": ["markup.quote.markdown"],
			"settings": {
				"foreground": "#A0AEC0",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["markup.list.markdown"],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"markup.link.markdown",
				"string.other.link.title.markdown",
				"string.other.link.description.markdown",
				"constant.other.reference.link.markdown"
			],
			"settings": {
				"foreground": "#7BB8F0",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"meta.object-literal.key",
				"support.type.property-name.json",
				"support.type.property-name.yaml"
			],
			"settings": {
				"foreground": "#C4A1F8"
			}
		},
		{
			"scope": [
				"constant.language.json",
				"string.quoted.double.json",
				"string.unquoted.plain.out.yaml"
			],
			"settings": {
				"foreground": "#7ADCA0"
			}
		},
		{
			"scope": ["meta.tag.js", "meta.tag.tsx", "meta.jsx.children"],
			"settings": {
				"foreground": "#7BB8F0"
			}
		},
		{
			"scope": ["support.class.component.js", "support.class.component.tsx"],
			"settings": {
				"foreground": "#A78DEA"
			}
		},
		{
			"scope": [
				"variable.language.php",
				"support.function.construct.php",
				"support.class.php"
			],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"meta.function-call.python",
				"meta.function-call.arguments.python"
			],
			"settings": {
				"foreground": "#7BB8F0"
			}
		},
		{
			"scope": [
				"entity.name.class.python",
				"entity.name.function.python",
				"meta.function.decorator.python"
			],
			"settings": {
				"foreground": "#A78DEA"
			}
		},
		{
			"scope": [
				"meta.embedded.block.js",
				"meta.embedded.expression.js",
				"meta.embedded.line.tsx",
				"meta.embedded.block.tsx",
				"meta.embedded.expression.tsx",
				"meta.jsx.children.tsx",
				"meta.tag.js",
				"meta.tag.tsx",
				"meta.tag.without-attributes.js",
				"meta.tag.without-attributes.tsx"
			],
			"settings": {
				"foreground": "#7BB8F0"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.jsx",
				"entity.other.attribute-name.tsx"
			],
			"settings": {
				"foreground": "#F8BB77"
			}
		},
		{
			"scope": [
				"variable.other.readwrite.cpp",
				"variable.other.readwrite.c",
				"variable.other.cpp",
				"variable.parameter.cpp",
				"variable.other.readwrite.java",
				"variable.parameter.java",
				"variable.other.readwrite.cs",
				"variable.parameter.cs",
				"variable.other.property.cs"
			],
			"settings": {
				"foreground": "#CBD5E0"
			}
		},
		{
			"scope": [
				"variable.language.this.cs",
				"variable.language.this.java",
				"variable.language.this.cpp"
			],
			"settings": {
				"foreground": "#A78DEA",
				"fontStyle": "italic"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"variable": "#E6ECF5",
		"parameter": "#F8BB77",
		"property": "#E6ECF5",
		"function": "#7BB8F0",
		"method": "#7BB8F0",
		"class": "#A78DEA",
		"interface": "#A78DEA",
		"enum": "#F87979",
		"typeParameter": "#A78DEA",
		"namespace": "#7BB8F0",
		"macro": "#F8BB77",
		"modifier": "#C4A1F8",
		"regexp": "#7ADCA0",
		"decorator": "#F87979",
		"punctuation": "#7BB8F0"
	}
}

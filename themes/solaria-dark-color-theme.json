  "tokenColors": [
    {
      "scope": [
        "keyword.control.go",
        "keyword.package.go",
        "keyword.import.go",
        "keyword.func.go",
        "keyword.struct.go",
        "keyword.interface.go"
      ],
      "settings": {
        "foreground": "#C4A1F8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["string.quoted.double.go", "string.quoted.raw.go"],
      "settings": {
        "foreground": "#68D391"
      }
    },
    {
      "scope": ["string.template", "string.interpolated"],
      "settings": {
        "foreground": "#68D391",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric.go",
      "settings": {
        "foreground": "#FFCA28"
      }
    },
    {
      "scope": "entity.name.function.go",
      "settings": {
        "foreground": "#7BB8F0"
      }
    },
    {
      "scope": "storage.type.go",
      "settings": {
        "foreground": "#C4A1F8"
      }
    },
    {
      "scope": ["comment.line.double-slash.go", "comment.block.go"],
      "settings": {
        "foreground": "#A0AEC0",
        "fontStyle": "italic"
      }

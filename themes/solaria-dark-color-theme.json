      "settings": {
        "foreground": "#A0AEC0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.other.symbol.ruby",
        "punctuation.section.block.begin.ruby",
        "punctuation.section.block.end.ruby"
      ],
      "settings": {
        "foreground": "#68D391"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#C4A1F8",
        "fontStyle": "bold"
      }
